<h2>Edit Historical Landmark</h2>

<form method="post" action="/landmarks/<%= @landmark.id %>">
  <input type="hidden" id="hidden" name="_method" value="patch">
  <input type="text" name="landmark[name]" value="landmark[name]">
  <input type="Text" name="landmark[year_completed]" value="landmark[year_completed]">
  <input type="submit" value="submit">
</form>

<a class="button" href="/figures">Landmarks</a>
